					<!--Net Equivalent Sabre host command: WPNET-->
					<!--Private equivalent Sabre host command: WPPV‡P2NEG-->
					<!--Public equivalent Sabre host command: WPPL-->
					<FareOptions Excursion="true" Net="false" Private="false" Public="false"/>
					<!--Optional-->
					<!--Repeat Factor=0-->
					<ItineraryOptions>
						<!--Optional-->
						<!--Repeat Factor=0-99-->
						<!--"Number" is used to instruct the system to break the fare at a specified segment.-->
						<!--Single equivalent Sabre host command: WPBF1-->
						<BreakFare Number="1"/>
						<!--Multiple equivalent Sabre host command: WPBF1/3-->
						<BreakFare Number="3"/>
						<!--Optional-->
						<!--Repeat Factor=0-99-->
						<!--"Number" is used to instruct the system to force a connection at a specified segment.-->
						<!--Equivalent Sabre host command: WPX1-->
						<ForceConnection Number="1"/>
						<ForceConnection Number="2"/>
						<!--Optional-->
						<!--Repeat Factor=0-99-->
						<!--"Number" is used to instruct the system to force a stopover at a specifed segment.-->
						<!--Equivalent Sabre host command: WPO2-->
						<ForceStopOver Number="2"/>
						<ForceStopOver Number="3"/>
						<!--Optional-->
						<!--Repeat Factor=0-99-->
						<!--"Number" is used to instruct the system to override a fare break.-->
						<!--Single Equivalent Sabre host command: WPNB1-->
						<OverrideBreak Number="1"/>
						<!--Multiple Equivalent Sabre host command: WPNB1/4-->
						<OverrideBreak Number="4"/>
						<!--Optional-->
						<!--Repeat Factor=0-99-->
						<!--"SegmentSelect" is used to instruct the system to price specified itinerary segments.-->
						<!--"RPH" is used to match up with ".../CommandPricing" or .../ItineraryOptions/GoverningCarrierOverride.-->
						<!--Equivalent Sabre host command: WPS1-->
						<SegmentSelect EndNumber="3" Number="1" RPH="1"/>
						<SegmentSelect EndNumber="6" Number="4" RPH="2"/>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--"SideTrip" is used to instruct the system to price the specified itinerary segments as a side trip.-->
						<!--Equivalent Sabre host command: WPST1-3-->
						<SideTrip EndNumber="3" Number="1"/>
					</ItineraryOptions>
					<!--Optional-->
					<!--Repeat Factor=0-99-->
					<!--"NameSelect" is used to instruct the system to price theitinerary based upon a particular name field.-->
					<!--Equivalent Sabre host command: WPN1.1-3.1-->
					<NameSelect NameNumber="1.1" EndNameNumber="3.1"/>
					<!--Equivalent Sabre host command: WPN1.1/4.1-->
					<NameSelect NameNumber="4.1"/>
					<!--Optional-->
					<!--Repeat Factor=0-99-->
					<!--.../NetRemit is ONLY applicable to BSP-based subscribers.-->
					<!--.../NetRemit is NOT applicable to ARC-based subscribers.-->
					<!--Equivalent Sabre host command: WPNET/700.00/CC900.00/V*ABC123‡P2NEG-->
					<NetRemit Code="ABC123">
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--"Code" is used to specify the CAR code.-->
						<!--Equivalent Sabre host command: WPNET/V*ABC123/C*DEF456‡P2NEG-->
						<CAR Code="DEF456"/>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--"Amount" is used to specify the amount of net cash.-->
						<!--"CurrencyCode" is used to specify the currency code of net cash.-->
						<!--Equivalent Sabre host command: WPNET/700.00/CC900.00/V*ABC123‡P2NEG-->
						<!--Equivalent Sabre host command without "Cash": WPNET/V*ABC123‡P2NEG-->
						<!--CurrencyCode equivalent Sabre host command: WPNET/GBP700.00/V*ABC123‡P2NEG-->
						<Cash Amount="700.00" CurrencyCode="GBP"/>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--"Amount" is used to specify the amount of credit.-->
						<!--Equivalent Sabre host command: WPNET/700.00/CC900.00/V*ABCDEF‡P2NEG-->
						<!--Equivalent Sabre host command without "Credit": WPNET/700.00/V*ABCDEF‡P2NEG-->
						<Credit Amount="900.00"/>
					</NetRemit>
					<!--Optional-->
					<!--Repeat Factor=0-->
					<Overrides>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--"FuelSurchargeOverride" is used to instruct the system to override applicable fuel surcharges.-->
						<!--Equivalent Sabre host command: WPNQF-->
						<FuelSurchargeOverride Ind="true"/>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--"RPH" is used to tie a governing carrier override to particular segments.  If a client application needs to utilize a governing carrier override from a particular segment or segments the application can use .../GoverningCarrierOverride in conjunction with .../ItineraryOptions/SegmentSelect-->
						<GoverningCarrierOverride RPH="1">
							<!--Mandatory-->
							<!--Repeat Factor=0-->
							<!--"Code" is used to instruct the system to override the governing carrier associated with the entire itinerary, a particular segment, or a range of segments.-->
							<!--Entire itinerary equivalent Sabre host command: WP*C-YY-->
							<!--Segment select equivalent Sabre host command: WPS1-3*C-YY-->
							<Airline Code="YY"/>
						</GoverningCarrierOverride>
						<!--Multiple segment select equivalent Sabre host command: WPS1-3*C-YY‡S4-6*C-XX-->
						<GoverningCarrierOverride RPH="2">
							<Airline Code="XX"/>
						</GoverningCarrierOverride>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--Cannot combine with "NoFareRestrictions."-->
						<!--"Ind" is used to instruct the system to price the itinerary with fares having no advance purchase requirements.-->
						<!--Equivalent Sabre host command: WPXA-->
						<NoAdvancePurchase Ind="false"/>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--Cannot combine with "NoAdvancePurchase," "NoMinMaxStay," or "NoPenalty."-->
						<!--"Ind" is used to instruct the system to price the itinerary with fares having no restrictions.-->
						<!--Equivalent Sabre host command: WPXR-->
						<NoFareRestrictions Ind="false"/>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--Cannot combine with "NoFareRestrictions."-->
						<!--"Ind" is used to instruct the system to price the itinerary with fares having no minimum/maximum stay requirements.-->
						<!--Equivalent Sabre host command: WPXS-->
						<NoMinMaxStay Ind="false"/>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--Cannot combine with "NoFareRestrictions."-->
						<!--"Ind" is used to instruct the system to price the itinerary with fares having no penalties.-->
						<!--Equivalent Sabre host command: WPXP-->
						<NoPenalty Ind="false"/>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--"Purchase" is used to instruct the system to override the default POS location, and price the itinerary as if the default POS location were located in the alternate, specified location-->
						<!--Equivalent Sabre host command: WPSPAR-->
						<Purchase LocationCode="PAR'"/>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--"Ticketing" is used to instruct the system to override the default ticketing location, and price the itinerary as if the default ticketing location were located in the alternate, specified location-->
						<!--Equivalent Sabre host command: WPTTPA-->
						<Ticketing LocationCode="'TPA"/>
					</Overrides>
					<!--Optional-->
					<!--Repeat Factor=0-->
					<!--"PassengerStatus" is used to is used to instruct the system to price the itinerary with a specified passenger status code.-->
					<!--Equivalent Sabre host command: WPEM/(country or state code), WPRY/(country or state code), WPNT/(country or state code)-->
					<!--Equivalent Sabre host command: WPEM/CA-->
					<PassengerStatus>EM/CA</PassengerStatus>
					<!--Optional-->
					<!--Repeat Factor=0-99-->
					<!--"Code" is used to specify a passenger type code.-->
					<!--"Force" is used to force the system to price with the specified passenger type code.  If the client requests multiple passenger types and also wishes to make use of @Force they should only pass it on the last .../PassengerType occurence.-->
					<!--"Quantity" is used to specify the number of passengers.-->
					<!--Code/Quantity equivalent Sabre host command: WPP2ADT-->
					<!--Force="true" equivalent Sabre host command: WPP2ADT‡XO-->
					<PassengerType Code="ADT" Force="true" Quantity="2"/>
					<!--Optional-->
					<!--Repeat Factor=0-->
					<!--"Amount" is used to specify an amount to add on top of the fare.-->
					<!--Equivalent Sabre host command: WPPU*100.00-->
					<PlusUp Amount="100.00">
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--Equivalent Sabre host command: WPPU*100.00/AD75-->
						<TicketDesignator>AD75</TicketDesignator>
					</PlusUp>
					<!--Optional-->
					<!--Repeat Factor=0-->
					<!--"Code" is used to specify a ship's country of registration.-->
					<!--This element is only to be used when .../PassengerType@Code="SEA".-->
					<!--Equivalent Sabre host command: WPPSEA‡SUSA-->
					<ShipsRegistry>USA</ShipsRegistry>
					<!--Optional-->
					<!--Repeat Factor=0-->
					<Taxes>
						<!--Optional-->
						<!--"Ind" is used to specify to price an itinerary without taxes.-->
						<!--Equivalent Sabre host command: WPTN-->
						<NoTax Ind="true"/>
						<!--Optional-->
						<!--Repeat Factor=0-99-->
						<!--"Code" is used to specify a tax code to exempt.-->
						<!--Equivalent Sabre host command: WPTE-UB-->
						<TaxExempt Code="UB"/>
						<TaxExempt Code="XY"/>
						<!--Optional-->
						<!--Repeat Factor=0-99-->
						<!--"TaxOverride" is used to specify a tax to override.-->
						<!--Equivalent Sabre host command: WPTX3.00XY/6.00YC-->
						<TaxOverride Amount="3.00" Code="XY"/>
						<TaxOverride Amount="6.00" Code="YC"/>
					</Taxes>
					<!--Optional-->
					<!--Repeat Factor=0-99-->
					<!--"ValidityDates" is used to specify ticket validity dates.-->
					<!--Equivalent Sabre host command: WPV1-3*21DEC24DEC-->
					<ValidityDates>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--"NotValidAfter" is used to specify the ending validity date.-->
						<!--"NotValidAfter" follows this format: MM-DD-->
						<NotValidAfter>12-24</NotValidAfter>
						<!--Optional-->
						<!--Repeat Factor=0-->
						<!--"NotValidBefore" is used to specify the beginning validity date.-->
						<!--"NotValidBefore" follows this format: MM-DD-->
						<NotValidBefore>12-21</NotValidBefore>
						<!--Mandatory-->
						<!--Repeat Factor=0-->
						<!--"Segment" is used to specify the relevant itinerary segments.-->
						<Segment EndNumber="3" Number="1"/>
					</ValidityDates>
				</PricingQualifiers>
			</OptionalQualifiers>
		</PriceRequestInformation>
	</OTA_AirPriceRQ>
	<!--Optional-->
	<!--Repeat Factor=0-->
	<!--HaltOnError" (if true) tells the system to halt processing if any errors are returned from the function being called thus cancelling any subsequent processing that was specified in the request message.-->
	<!--"IgnoreAfter" is used to perform an ignore to clear out the AAA.  Please note that it does not close the session.-->
