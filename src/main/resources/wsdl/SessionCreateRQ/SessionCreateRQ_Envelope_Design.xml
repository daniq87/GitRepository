<?xml version="1.0" encoding="UTF-8"?>
<SOAP-ENV:Envelope xmlns:SOAP-ENV="http://schemas.xmlsoap.org/soap/envelope/" xmlns:eb="http://www.ebxml.org/namespaces/messageHeader" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsd="http://www.w3.org/1999/XMLSchema">
	<SOAP-ENV:Header>
		<!--Mandatory-->
		<!--Repeat Factor=0-->
		<eb:MessageHeader SOAP-ENV:mustUnderstand="1" eb:version="1.0">
			<!--Mandatory-->
			<!--Repeat Factor=0-->
			<eb:From>
				<!--Mandatory-->
				<!--Repeat Factor=0-->
				<!--"eb:PartyId" is used to specify the client application that is sending the message.-->
				<!--Maximum length = 255-->
				<eb:PartyId type="urn:x12.org:IO5:01">999999</eb:PartyId>
			</eb:From>
			<!--Mandatory-->
			<!--Repeat Factor=0-->
			<eb:To>
				<!--Mandatory-->
				<!--Repeat Factor=0-->
				<!--"eb:PartyId" is used to specify the client application that the message is being sent to.-->
				<!--Maximum length = 255-->
				<eb:PartyId type="urn:x12.org:IO5:01">123123</eb:PartyId>
			</eb:To>
			<!--Mandatory-->
			<!--Repeat Factor=0-->
			<!--"eb:CPAId" is used to specify the client application's organization, i.e. PseudoCityCode, or airline code.-->
			<!--Maximum length = 20-->
			<eb:CPAId>IPCC</eb:CPAId>
			<!--Mandatory-->
			<!--Repeat Factor=0-->
			<!--"eb:ConversationId" is used to identify the set of related messages that make up a connection and its associated session.-->
			<!--Maximum length = 255-->
			<eb:ConversationId>webservices.support@sabre.com</eb:ConversationId>
			<!--Mandatory-->
			<!--Repeat Factor=0-->
			<!--"eb:Service" is used to identify the service that acts on the message.-->
			<!--Maximum length = 128-->
			<eb:Service eb:type="OTA">SessionCreateRQ</eb:Service>
			<!--Mandatory-->
			<!--Repeat Factor=0-->
			<!--"eb:Action" is used to identify the action that acts on the service.-->
			<!--Maximum length = 255-->
			<eb:Action>SessionCreateRQ</eb:Action>
			<!--Mandatory-->
			<!--Repeat Factor=0-->
			<eb:MessageData>
				<!--Mandatory-->
				<!--Repeat Factor=0-->
				<!--"eb:MessageId" is used as a globally unique identifier for each message.-->
				<!--Maximum length = 255-->
				<eb:MessageId>1000</eb:MessageId>
				<!--Mandatory-->
				<!--Repeat Factor=0-->
				<!--"eb:Timestamp" is used to represent the time that the message header was created conforming to a date-time.-->
				<!--Maximum length = 30-->
				<eb:Timestamp>2012-12-21T00:00:00Z</eb:Timestamp>
			</eb:MessageData>
		</eb:MessageHeader>
		<!--Mandatory-->
		<!--Repeat Factor=0-->
		<wsse:Security xmlns:wsse="http://schemas.xmlsoap.org/ws/2002/12/secext" xmlns:wsu="http://schemas.xmlsoap.org/ws/2002/12/utility">
			<!--Mandatory-->
			<!--Repeat Factor=0-->
			<wsse:UsernameToken>
				<!--Mandatory-->
				<!--Repeat Factor=0-->
				<!--"wsse:Username" is used to specify the client application's user name.-->
				<!--Maximum length = 20-->
				<wsse:Username>12345</wsse:Username>
				<!--Mandatory-->
				<!--Repeat Factor=0-->
				<!--"wsse:Password" is used to specify the client application's password.-->
				<!--Maximum length = 30-->
				<wsse:Password>XXXXXXXX</wsse:Password>
				<!--Opional-->
				<!--Repeat Factor=0-->
				<!--"wsse:Password" is used to specify a new client application password.-->
				<!--Maximum length = 30-->
				<NewPassword>YYYYYYYY</NewPassword>
				<!--Mandatory-->
				<!--Repeat Factor=0-->
				<!--"Organization" is used to specify the client application's organization, i.e. PseudoCityCode, or airline code.-->
				<!--Maximum length = 20-->
				<Organization>IPCC</Organization>
				<!--Mandatory-->
				<!--Repeat Factor=0-->
				<!--"Domain" is used to specify the client application's domain, i.e. "DEFAULT" for TN-based subscribers, or the particular airline code for SabreSonic-based subscribers.-->
				<!--Maximum length = 20-->
				<Domain>DEFAULT</Domain>
				<!--Optional-->
				<!--Repeat Factor=0-->
				<!--Maximum length = 6-->
				<Lniata>ABC123</Lniata>
				<!--Optional-->
				<!--Repeat Factor=0-->
				<!--Maximum length = 10-->
				<LockId>DEF456</LockId>
			</wsse:UsernameToken>
		</wsse:Security>
	</SOAP-ENV:Header>
	<!--Mandatory-->
	<!--Repeat Factor=0-->
	<SOAP-ENV:Body>
		<!--Optional-->
		<!--Repeat Factor=0-->
		<!--"returnContextID" when "true" instructs the system to return the extended Context ID.  The extended Context ID helps the system to return the most accurate response-related data.  It is recommended that users who change AAA and also utilize the FareLLSRQ, OTA_AirLowFareSearchLLSRQ, OTA_AirPriceLLSRQ, or OTA_AirRulesLLSRQ services leverage the extended Context ID feature since context is critical when it comes to these services.-->
		<SessionCreateRQ returnContextID="true">
			<!--Optional-->
			<!--Repeat Factor=0-->
			<POS>
				<!--Mandatory-->
				<!--Repeat Factor=0-->
				<!--"Organization" is used to specify the client application's organization, i.e. PseudoCityCode, or airline code.-->
				<Source PseudoCityCode="IPCC"/>
			</POS>
		</SessionCreateRQ>
	</SOAP-ENV:Body>
</SOAP-ENV:Envelope>
