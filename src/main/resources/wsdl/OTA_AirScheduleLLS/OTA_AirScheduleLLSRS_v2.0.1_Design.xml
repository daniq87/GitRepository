			</stl:SystemSpecificResults>
		</stl:Success>
		<!--Optional-->
		<!--Repeat Factor=0-99-->
		<!--"type" is used to return the error type.  Values are limited to: "Application," or "BusinessLogic." "Transport" and "Validation" type errors are returned as soap-fault content.-->
		<stl:Error type="BusinessLogic" timeStamp="2013-06-17T15:00:00-06:00">
			<!--Optional-->
			<!--Repeat Factor=0-->
			<stl:SystemSpecificResults>
				<!--Optional-->
				<!--Repeat Factor=0-99-->
				<!--"HostCommand" is used to return the command associated with the request transaction if applicable.-->
				<!--"LNIATA" is used to return the line address associated with the particular host command.-->
