				<!--"Message" is used to return the warning message.-->
				<stl:Message code="0">TEST</stl:Message>
				<!--Optional-->
				<!--Repeat Factor=0-->
				<!--"ShortText" is used to return additional warning message-related information. -->
				<stl:ShortText>TEST</stl:ShortText>
			</stl:SystemSpecificResults>
		</stl:Warning>		
	</stl:ApplicationResults>
	<!--Optional-->
	<!--Repeat Factor=0-->
	<!--"DestinationTimeZone" is used to return the time zone code for the destination location in terms of GMT plus or minus.-->
	<!--"OriginTimeZone" is used to return the time zone code for the origin location in terms of GMT plus or minus.-->
	<OriginDestinationOptions DestinationTimeZone="-480" OriginTimeZone="-360">
		<!--Optional-->
		<!--Repeat Factor=0-999-->
		<!--"RPH" is used as a reference place holder.-->
		<OriginDestinationOption RPH="1">
			<!--Mandatory-->
			<!--Repeat Factor=0-999-->
			<!--"ArrivalDateTime" is used to return the time and date of flight arrival.-->
			<!--"ArrivalDateTime" follows this format: MM-DDTHH:MM-->
			<!--"ConnectionInd" is used to return the connection indicator associated with the particular flight segment.-->
			<!--"DepartureDateTime" is used to return the time and date of flight departure.-->
			<!--"DepartureDateTime" follows this format: MM-DDTHH:MM-->
			<!--"eTicket" is used to return the flight's electronic ticket eligibility.-->
			<!--"FlightNumber" is used to return the flight number.-->
			<!--"DOT_Ind" is used to return the flight's on time rate.-->
			<!--"RPH" is used as a reference place holder.-->
			<!--"SmokingAllowed" is used to indicate whether or not smoking is permitted on the flight.-->
			<!--"StopQuantity" is used to return the number of stops the flight is scheduled to make.-->
			<FlightSegment ArrivalDateTime="12-24T21:13" ConnectionInd="O" DepartureDateTime="12-24T13:00" DOT_Ind="2" eTicket="true" FlightNumber="1234" RPH="1" SmokingAllowed="false" StopQuantity="0">
				<!--Optional-->
				<!--Repeat Factor=0-->
				<!--"LocationCode" is used to return the departure airport code.-->
				<OriginLocation LocationCode="FSG"/>
				<!--Optional-->
				<!--Repeat Factor=0-->
				<!--"LocationCode" is used to return the arrival airport code.-->
				<DestinationLocation LocationCode="DBM"/>
				<!--Optional-->
				<!--Repeat Factor=0-->
				<!--"Code" is used to return the operating airline code.-->
				<OperatingAirline Code="XX"/>
				<!--Optional-->
				<!--Repeat Factor=0-->
				<!--"Code" is used to return the disclosure airline code.-->
				<!--"CompanyShortName" is used to return the disclosure airline name.-->
				<DisclosureAirline Code="ZZ" CompanyShortName="TEST">
					<!--Optional-->
					<!--Repeat Factor=0-999-->
					<!--"Text" is used to return miscellaneous disclosure carrier-related text.-->
					<Text>TEST0</Text>
					<Text>TEST1</Text>
				</DisclosureAirline>
				<!--Optional-->
				<!--Repeat Factor=0-->
				<!--"AirEquipType" is used to return aircraft equipment information/type.-->
				<Equipment AirEquipType="737"/>
				<!--Optional-->
				<!--Repeat Factor=0-->
				<!--"Code" is used to return the marketing airline code.-->
				<!--"FlightNumber" is used to return the marketing airline flight number.-->
				<!--"ParticipationLevel" is used to return the marketing airline's level of participation in the Sabre host system.-->
				<MarketingAirline Code="AA" FlightNumber="1234" ParticipationLevel="DC"/>
				<!--Optional-->
				<!--Repeat Factor=0-99-->
				<!--"MealCode" is used to return applicable meal codes served on the flight.-->
				<Meal MealCode="B"/>
				<Meal MealCode="B"/>
				<!--Optional-->
				<!--Repeat Factor=0-->
				<!--"Canceled" is used to indicate whether or not the flight is canceled.-->
				<!--"Charter" is used to indicate whether or not the flight is a charter flight.-->
				<!--"CodeshareBlockDisplay" is used to return the codeshare block display code if applicable.-->
				<!--"DiscontinueDate" is used to return the date that the availability will be discontinued.-->
				<!--"DiscontinueDate" follows this format: MM-DD-->
				<!--"EffectiveDate" is used to return the date that the availability is effective.-->
				<!--"EffectiveDate" follows this format: MM-DD-->
				<!--"TrafficRestriction" is used to return traffic restriction codes if applicable.-->
				<FlightDetails Canceled="false" Charter="false" CodeshareBlockDisplay="ABC123" DiscontinueDate="12-24" EffectiveDate="12-21" TrafficRestrictionCode="DEF456">
					<!--Optional-->
					<!--Repeat Factor=0-99-->
					<!--"Text" is used to return miscellaneous free text.-->
					<Text>TEST0</Text>
					<Text>TEST1</Text>
				</FlightDetails>
				<!--Optional-->
				<!--Repeat Factor=0-99-->
				<!--"Availability" is used to return the number of available seats applicable to the specific booking class of service.-->
				<!--"BrandID" is used to return the brand ID associated with the particular booking class of service if applicable.-->
				<!--"ResBookDesigCode" is used to return the booking class of service.-->
				<!--"RPH" is used as a reference place holder.-->
				<BookingClassAvail Availability="0" BrandID="XX" ResBookDesigCode="F" RPH="1"/>
				<BookingClassAvail Availability="8" BrandID="XX" ResBookDesigCode="Y" RPH="1"/>
				<!--Optional-->
				<!--Repeat Factor=0-->
				<DaysOfOperation>
					<!--Mandatory-->
					<!--Repeat Factor=0-->
					<OperationSchedule>
						<!--Mandatory-->
						<!--Repeat Factor=0-->
						<OperationTimes>
							<!--Mandatory-->
							<!--Repeat Factor=0-->
							<!--"OperationTime" is used to indicate whether or not a flight operates on a particular week day.-->
							<OperationTime Fri="true" Mon="true" Sat="true" Sun="true" Thur="true" Tue="true" Weds="true"/>
						</OperationTimes>
					</OperationSchedule>
				</DaysOfOperation>
			</FlightSegment>
		</OriginDestinationOption>
	</OriginDestinationOptions>
</OTA_AirScheduleRS>
