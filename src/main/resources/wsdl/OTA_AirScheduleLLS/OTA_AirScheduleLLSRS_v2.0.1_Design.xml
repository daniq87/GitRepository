		<!--Optional-->
		<!--Repeat Factor=0-99-->
		<!--"type" is used to return the warning type.  Values are limited to: "BusinessLogic." No other types are currently supported.-->
		<stl:Warning type="BusinessLogic">
			<!--Optional-->
			<!--Repeat Factor=0-->
			<stl:SystemSpecificResults>
